{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^u--(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^up-(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "^u-([^-].*)": "uview-plus/components/u-$1/u-$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/tab/home/index",
      "style": {
        "navigationBarTitleText": "首页",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/tab/choice/index",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/tab/user/index",
      "style": {
        "navigationStyle": "custom"
      }
    }
  ],
  "subPackages": [
    {
      "root": "pages/common",
      "pages": [
        {
          "path": "login/index",
          "style": {
            "navigationStyle": "custom"
          }
        },
        {
          "path": "webview/index",
          "navigationBarTitleText": "网页"
        }
      ]
    },
    {
      "root": "pages/intention",
      "pages": [{
        "path": "detail/index",
        "style": {
          "navigationStyle": "custom"
        }
      }, {
        "path": "collage/list",
        "style": {
          "navigationStyle": "custom"
        }
      }, {
        "path": "collage/detail",
        "style": {
          "navigationStyle": "custom"
        }
      }, {
        "path": "major/list",
        "style": {
          "navigationStyle": "custom"
        }
      }, {
        "path": "compare/list",
        "style": {
          "navigationStyle": "custom"
        }
      }, {
        "path": "compare/school",
        "style": {
          "navigationStyle": "custom"
        }
      }, {
        "path": "compare/detail",
        "style": {
          "navigationStyle": "custom"
        }
      }, {
        "path": "rank/index",
        "style": {
          "navigationStyle": "custom"
        }
      }]
    },
    {
      "root": "pages/code",
      "pages": [{
        "path": "index",
        "style": {
          "navigationStyle": "custom"
        }
      }]
    },
    {
      "root": "pages/choice",
      "pages": [{
        "path": "index",
        "style": {
          "navigationStyle": "custom"
        }
      }]
    },
    {
      "root": "pages/user",
      "pages": [{
        "path": "attention/index",
        "style": {
          "navigationStyle": "custom"
        }
      }, {
        "path": "setting/index",
        "style": {
          "navigationStyle": "custom"
        }
      }, {
        "path": "report/index",
        "style": {
          "navigationStyle": "custom"
        }
      }]
    }
  ],
  "preloadRule": {
    "pages/tab/home/index": {
      "network": "all",
      "packages": ["pages/common"]
    }
  },
  "tabBar": {
    "color": "#7A7E83",
    "selectedColor": "#E94650",
    "borderStyle": "black",
    "backgroundColor": "#ffffff",
    "list": [{
      "pagePath": "pages/tab/home/index",
      "text": "首页",
      "iconPath": "static/images/home.png",
      "selectedIconPath": "static/images/home-active.png"
    }, {
      "pagePath": "pages/tab/choice/index",
      "text": "择校",
      "iconPath": "static/images/zx.png",
      "selectedIconPath": "static/images/zx-active.png"
    }, {
      "pagePath": "pages/tab/user/index",
      "text": "我的",
      "iconPath": "static/images/my.png",
      "selectedIconPath": "static/images/my-active.png"
    }]
  },
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  }
}
